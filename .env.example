# QuickEdu Environment Variables
# Copy this file to .env and fill in your values

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# Cashfree Payment Gateway
VITE_CASHFREE_APP_ID=your_cashfree_app_id_here
VITE_CASHFREE_SECRET_KEY=your_cashfree_secret_key_here
VITE_CASHFREE_ENV=sandbox

# Site Configuration
VITE_SITE_NAME=QuickEdu
VITE_SITE_URL=https://quickedu.org.in
VITE_CONTACT_EMAIL=info@quickedu.org.in
VITE_CONTACT_PHONE=+919392328940

# Analytics
VITE_GA_TRACKING_ID=G-XXXXXXXXXX
VITE_GTM_ID=GTM-XXXXXXX

# Error Tracking (Sentry)
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_SENTRY_ENVIRONMENT=production

# Social Media
VITE_FACEBOOK_URL=https://facebook.com/quickedu
VITE_TWITTER_URL=https://twitter.com/quickedu
VITE_LINKEDIN_URL=https://linkedin.com/company/quickedu
VITE_INSTAGRAM_URL=https://instagram.com/quickedu

# Email Service (SendGrid/AWS SES)
VITE_EMAIL_SERVICE_KEY=your_email_service_key_here

# Admin Credentials (Change in production!)
VITE_ADMIN_USERNAME=admin
VITE_ADMIN_PASSWORD=QuickEdu@2024

# Feature Flags
VITE_ENABLE_CART=true
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_ANALYTICS=true

# CDN Configuration
VITE_CDN_URL=https://cdn.quickedu.org.in

# Security
VITE_ENABLE_HTTPS=true
VITE_ENABLE_CORS=true
